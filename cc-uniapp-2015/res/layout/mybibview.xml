                        android:layout_gravity="center"
                        android:layout_marginBottom="10dp"
                        android:background="@android:color/transparent"
                        android:onClick="navigateTo"
                        android:scaleType="fitCenter"
                        android:src="@drawable/mapsicon"/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
