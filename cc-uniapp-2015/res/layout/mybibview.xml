                        android:id="@+id/navigateToHopla"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_gravity="center"
                        android:layout_marginBottom="10dp"
                        android:background="@android:color/transparent"
                        android:onClick="navigateTo"
                        android:scaleType="fitCenter"
                        android:src="@drawable/mapsicon"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="17dp"
            android:background="@drawable/meallistitembackground"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_gravity="center_horizontal|center_vertical"
                android:background="@drawable/mealcatagorycolorblue"
                android:scaleType="fitCenter"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="2dp">

                <TextView
                    android:id="@+id/bib_bgp"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:clickable="true"
                    android:gravity="center_horizontal"
                    android:onClick="toggle_contents"
                    android:paddingLeft="@dimen/activity_horizontal_margin"
                    android:paddingRight="@dimen/activity_horizontal_margin"
                    android:text="@string/bib_bgp_titel"
                    android:textAppearance="?android:attr/textAppearanceLarge"/>

                <LinearLayout
                    android:id="@+id/bib_bgp_content"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/bib_bgp_address"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bib_bgp_address"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:id="@+id/bib_bgp_description"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
