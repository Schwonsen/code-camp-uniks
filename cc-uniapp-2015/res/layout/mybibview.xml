                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bib_contact"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:id="@+id/bib_kunst_tel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:autoLink="phone"
                        android:text="@string/bib_kunst_tel"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:id="@+id/bib_kunst_email"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:autoLink="web|email"
                        android:text="@string/bib_kunst_email"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bib_opening_time_label"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:id="@+id/bib_kunst_opening_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bib_kunst_opening_time"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <ImageButton
