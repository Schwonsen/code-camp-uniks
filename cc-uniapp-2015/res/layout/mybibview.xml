                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bib_contact"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:id="@+id/bib_hopla_tel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:autoLink="phone"
                        android:text="@string/bib_hopla_tel"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:id="@+id/bib_hopla_email"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:autoLink="web|email"
                        android:text="@string/bib_hopla_email"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bib_opening_time_label"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:id="@+id/bib_hopla_opening_time_bibgeb_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bib_hopla_opening_time_bibgeb_title"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:id="@+id/bib_hopla_opening_time_bibgeb"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bib_hopla_opening_time_bibgeb"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:id="@+id/bib_hopla_opening_time_service_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bib_hopla_opening_time_service_title"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:id="@+id/bib_hopla_opening_time_service"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bib_hopla_opening_time_service"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:id="@+id/bib_hopla_opening_time_nutzerservice_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bib_hopla_opening_time_nutzerservice_title"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

                    <TextView
                        android:id="@+id/bib_hopla_opening_time_nutzerservice"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bib_hopla_opening_time_nutzerservice"
                        android:textAppearance="?android:attr/textAppearanceMedium"/>

